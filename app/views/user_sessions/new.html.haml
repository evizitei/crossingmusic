.left 
  %div{:style=>"clear:both"}
    %h1 Login Here
    =form_for @user_session, :url => { :action => "create" } do |f|
      %fieldset.user_session
        =f.label :email,"Email:"
        =f.text_field :email
        %br
        =f.label :password,"Password:"
        =f.password_field :password
      =f.submit "Login"